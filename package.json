{
  "name": "fireblog",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build-dev": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "gc-deploy": "gcloud builds submit --tag gcr.io/fireblog-io/fireblog --timeout=3600",
    "dev:ssr": "ng run fireblog:serve-ssr",
    "serve:ssr": "node dist/fireblog/server/main.js",
    "build:ssr": "ng build && ng run fireblog:server",
    "prod": "npm run build:ssr && npm run serve:ssr",
    "prerender": "ng run fireblog:prerender",
    "lint": "ng lint",
    "config": "node src/environments/set-env.js",
    "vercel-build": "npm run config && npm run build:ssr"
  },
  "engines": {
    "node": "14"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.3.1",
    "@angular/cdk": "^13.3.2",
    "@angular/common": "~13.3.1",
    "@angular/compiler": "~13.3.1",
    "@angular/core": "^13.3.1",
    "@angular/fire": "^7.3.0",
    "@angular/forms": "~13.3.1",
    "@angular/material": "^13.3.2",
    "@angular/platform-browser": "~13.3.1",
    "@angular/platform-browser-dynamic": "~13.3.1",
    "@angular/platform-server": "~13.3.1",
    "@angular/router": "~13.3.1",
    "@angular/service-worker": "~13.3.1",
    "@nguniversal/express-engine": "^13.1.0",
    "express": "^4.15.2",
    "firebase": "^9.6.6",
    "ngx-markdown": "^12.0.1",
    "rxfire": "^6.0.0",
    "rxjs": "~7.4.0",
    "tslib": "^2.2.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/architect": "^0.1303.1",
    "@angular-devkit/build-angular": "^13.3.1",
    "@angular-eslint/builder": "13.2.0",
    "@angular-eslint/eslint-plugin": "13.2.0",
    "@angular-eslint/eslint-plugin-template": "13.2.0",
    "@angular-eslint/schematics": "13.2.0",
    "@angular-eslint/template-parser": "13.2.0",
    "@angular/cli": "^13.3.1",
    "@angular/compiler-cli": "~13.3.1",
    "@nguniversal/builders": "^13.1.0",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.20.38",
    "@typescript-eslint/eslint-plugin": "5.3.0",
    "@typescript-eslint/parser": "5.3.0",
    "eslint": "^7.32.0",
    "firebase-admin": "^8.10.0",
    "firebase-functions": "^3.6.0",
    "firebase-functions-test": "^0.2.2",
    "firebase-tools": "^9.19.0",
    "fuzzy": "^0.1.3",
    "inquirer": "^6.2.2",
    "inquirer-autocomplete-prompt": "^1.0.1",
    "jasmine-core": "~3.7.0",
    "jsonc-parser": "^3.0.0",
    "karma": "^6.3.16",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "open": "^7.0.3",
    "typescript": "~4.4.4"
  }
}
